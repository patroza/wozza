{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "outputs": ["build/**"],
      "dependsOn": ["^build"]
    },
    "test": {
      "cache": false,
      "dependsOn": []
    },
    "check": {
      "dependsOn": []
    },
    "update:latest": {
      "dependsOn": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
